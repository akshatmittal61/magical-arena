{
	"compilerOptions": {
		"incremental": true,
		"target": "es6",
		"module": "CommonJS",
		"allowJs": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"skipLibCheck": true
	}
}